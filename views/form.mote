<!-- IMPORTANT:  FORM's enctype must be "multipart/form-data" -->
<h1> File upload </h1>
<p>
Upload multiple .docx files simultaneously, then submit.
</p>
<form method="post" action="/" enctype="multipart/form-data">
  <div class="form-group">
    <label for="filesToUpload">Files:</label>
    <input name="filesToUpload[]" id="filesToUpload" type="file" multiple="" accept=".docx" />
  </div>
  <input type="submit">
</form>